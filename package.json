{
  "name": "september-green",
  "version": "1.0.0",
  "description": "eslint template for react",
  "main": "index.js",
  "devDependencies": {
    "@babel/cli": "^7.11.5",
    "@babel/core": "^7.11.5",
    "@babel/plugin-syntax-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "babel-loader": "^8.1.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.13",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx ./src --fix",
    "format": "prettier --write 'src/**/*.js'",
    "webpack": "npx webpack --config webpack.config.js",
    "watch": "npx webpack --watch --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hidekingerz/september-green.git"
  },
  "author": "Hidekingerz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hidekingerz/september-green/issues"
  },
  "homepage": "https://github.com/hidekingerz/september-green#readme"
}
